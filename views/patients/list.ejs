<!DOCTYPE html>
<html>
<head>
  <title>Patients List</title>
</head>
<body>
  <h1>Patients List</h1>

  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Date of Birth</th>
        <th>Gender</th>
        <th>Address</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% patients.forEach(patient => { %>
        <tr>
          <td><%= patient.id %></td>
          <td><%= patient.firstName %></td>
          <td><%= patient.lastName %></td>
          <td><%= patient.email %></td>
          <td><%= patient.phone %></td>
          <td><%= patient.dateOfBirth %></td>
          <td><%= patient.gender %></td>
          <td><%= patient.address %></td>
          <td>
            <a href="/patients/edit/<%= patient.id %>">Edit</a>
            <a href="/patients/delete/<%= patient.id %>">Delete</a>
          </td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>